{
  "expo": {
    "name": "Fingrow",
    "slug": "fingrow-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#09090b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fingrow.mobile"
    },
    "android": {
      "backgroundColor": "#09090b",
      "package": "com.fingrow.mobile",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "webpack"
    },
    "cli": {
      "appVersionSource": "remote"
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "plugins": [
      "expo-camera"
    ]
  }
}